/**
 * 4-way demultiplexor:
 * {a, b, c, d} = {in, 0, 0, 0} if sel == 00
 *                {0, in, 0, 0} if sel == 01
 *                {0, 0, in, 0} if sel == 10
 *                {0, 0, 0, in} if sel == 11
 */

CHIP DMux4Way {
    IN in, sel[2];
    OUT a, b, c, d;
   
    PARTS:
    DMux(in=in, sel=sel[0], a=top, b=bottom);

    DMux(in=top, sel=sel[1], a=a, b=c);
    DMux(in=bottom, sel=sel[1], a=b, b=d);
 }